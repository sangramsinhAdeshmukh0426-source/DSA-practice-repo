class Solution{
    static int removeDuplicate(int[] array){
        if(array.length==0)return 0;
        int i=0;
        for(int j=1;j<array.length;j++){
            if(array[j]!=array[i]){
                i++;
                array[i]=array[j];
                
            }
        }
        
        return i+1;
        
    }
